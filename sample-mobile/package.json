{
  "name": "mobile-client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:dev": "ENVFILE=../config/.env.development yarn android",
    "android:stage": "ENVFILE=../config/.env.uat yarn android",
    "android:live": "ENVFILE=../config/.env.production concurrently 'yarn start' 'react-native run-android --appId com.redibs --variant=liveDebug'",
    "ios": "react-native run-ios",
    "ios:debug": "yarn ios --scheme Debug",
    "ios:dev": "yarn ios --scheme Dev",
    "ios:qa": "yarn ios --scheme Sample QA",
    "ios:uat": "yarn ios --scheme Sample UAT",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --cache --cache-location ../../.cache/eslint/mobile.eslintcache",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "clean": "rm -rf ios/build android/build android/app/build",
    "pod": "cd ios && pod install",
    "bundle-ios": "react-native bundle --dev false --entry-file index.js --bundle-output ios/main.jsbundle --platform ios",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "pod-repo-update": "cd ios && pod install --repo-update",
    "jetify": "jetify",
    "deploy-testflight": "yarn bundle-ios && cd ios && fastlane beta",
    "android:bundle": "cd android ./gradlew bundleRelease -x bundleReleaseJsAndAssets",
    "android:apk": "cd android ./gradlew assembleRelease -x bundleReleaseJsAndAssets",
    "android:qa": "cd android && bundle exec fastlane android qa --env qa",
    "ios:deploy:qa": "cd ios && bundle exec fastlane qa",
    "ios:deploy:uat": "cd ios && bundle exec fastlane uat",
    "android:uat": "cd android && bundle exec fastlane android uat --env uat"
  },
