{
  "name": "web-client",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:uat": "tsc && vite build --mode uat",
    "build:qa": "tsc && vite build --mode qa",
    "build:gcpdev": "tsc && vite build --mode gcpdev",
    "clean": "rimraf --no-glob ./dist ./tsconfig.tsbuildinfo ./node_modules/.vite",
    "serve": "vite preview",
    "test": "yarn test-unit",
    "test-unit": "echo 'No test present yet'",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --cache --cache-location ../../.cache/eslint/vite-app.eslintcache",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx --fix"
  },
  "dependencies": {
    "@mapbox/search-js-react": "1.0.0-beta.13",
    "@apollo/client": "^3.6.9",
    "@honkhonk/vite-plugin-svgr": "1.1.0",
    "@auto/quote-client": "1.0.0",
    "@marker.io/browser": "^0.16.0",
    "@stripe/react-stripe-js": "^1.9.0",
    "@stripe/stripe-js": "^1.32.0",
    "aphrodite": "^2.4.0",
    "classnames": "^2.3.1",
    "firebase": "^9.9.0",
    "hellosign-embedded": "^2.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "6.7.0",
    "stripe": "^10.14.0",
    "rudder-sdk-js": "^2.20.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@emotion/babel-plugin": "11.9.2",
    "@sample/eslint-config-bases": "1.0.0",
    "@types/hellosign-embedded": "^2.0.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "@vitejs/plugin-react": "1.3.2",
    "eslint": "8.17.0",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "typescript": "4.7.3",
    "vite": "2.9.9",
    "vite-tsconfig-paths": "3.5.0"
  }
}
